
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl:  <http://www.w3.org/2002/07/owl#>

# Lista los tipos de cada individuo en el grafo LOV
SELECT DISTINCT ?individual ?type
WHERE {
  GRAPH <LOV_DATASET_URI> {
    ?individual rdf:type ?type .
    # Opcional: filtrar para excluir clases OWL/RDFS si no quieres ontolog√≠as
    FILTER(isIRI(?individual))
    FILTER(isIRI(?type))
  }
}
ORDER BY ?individual
